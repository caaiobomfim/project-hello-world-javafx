<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.helloworldjavafx.gui.ViewController">
   <children>
      <ComboBox fx:id="comboBoxPerson" layoutX="14.0" layoutY="14.0" onAction="#onComboBoxPersonAction" prefWidth="150.0" promptText="Select a person..." />
      <Button fx:id="btnAll" layoutX="176.0" layoutY="15.0" mnemonicParsing="false" onAction="#onBtnAllAction" text="All" />
   </children>
</AnchorPane>
